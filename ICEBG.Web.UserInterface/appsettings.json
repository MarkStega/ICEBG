{
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "post:/api/CspReporting/UriReport",
        "Period": "10s",
        "Limit": 2
      }
    ]
  },
  "ClientRateLimitPolicies": {
    "ClientRules": [
      {
        "ClientId": "icebg",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "10s",
            "Limit": 2
          }
        ]
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ICEBG": {
    "BaseConfiguration": {
      "ConfigurationIdentifier": "ICEBG",
      "DetailedErrors": "true",
      "GrpcEndpointPrefix": "https://T570:7173/",
      "SqlConnectionString": "n/a",
      "WeatherEndpoint": "https://T570:7173/WeatherForecast"
    },
    "DetailedErrors": "true"
  }
