{
  "name": "icebg",
  "version": "1.0.0",
  "scripts": {
    "build-css": "sass --no-color --unicode --load-path=node_modules --no-source-map WebAssets/Styles/icebg.scss wwwroot/css/icebg.css",
    "build-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map WebAssets/Styles/icebg.scss wwwroot/css/icebg.min.css",
    "build-js": "terser wwwroot/js/temp.js --beautify --output wwwroot/js/icebg.js",
    "build-min-js": "terser wwwroot/js/temp.js --compress --mangle --ecma 2016 --output wwwroot/js/icebg.min.js",
    "build-scripts": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-object-rest-spread": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "babel-loader": "^9.1.3",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "material-components-web": "14.0.0",
    "regexp": "1.0.0",
    "sass": "1.39.2",
    "source-map-loader": "4.0.1",
    "terser": "^5.20.0",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0"
  }
}
