{
  "name": "icebg",
  "version": "1.0.0",
  "scripts": {
    "build-css": "sass --no-color --unicode --load-path=node_modules --no-source-map WebAssets/Styles/icebg.scss wwwroot/css/icebg.css",
    "build-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map WebAssets/Styles/icebg.scss wwwroot/css/icebg.min.css",
    "build-js": "webpack --config webpack.config.js",
    "build-min-js": "terser wwwroot/js/icebg.js --compress --mangle --ecma 2016 --output wwwroot/js/icebg.min.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "babel-loader": "^9.1.3",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "material-components-web": "14.0.0",
    "regexp": "1.0.0",
    "sass": "1.39.2",
    "source-map-loader": "4.0.1",
    "terser": "^5.27.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
